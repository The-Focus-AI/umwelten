    "chalk": "^5.4.1",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.0",
    "file-type": "^21.0.0",
    "ollama-ai-provider": "^1.2.0",
    "ora": "^8.2.0",
    "tsx": "^4.20.3",
    "uuid": "^11.1.0",
    "zod": "^3.25.67",
    "zod-to-json-schema": "^3.24.5"
  },
  "devDependencies": {
