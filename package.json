{
  "name": "umwelten",
  "version": "0.3.3",
  "type": "module",
  "bin": {
    "umwelten": "./dist/cli/cli.js"
  },
  "description": "A tool for systematically evaluating language models",
  "scripts": {
    "test": "vitest --reporter=verbose",
    "test:run": "vitest run --reporter=verbose",
    "test:watch": "vitest --reporter=verbose",
    "test:coverage": "vitest run --coverage --reporter=verbose",
    "lint": "eslint src --ext .ts,.tsx",
    "build": "tsc",
    "cli": "tsx src/cli/cli.ts",
    "prepublishOnly": "npm run build",
    "prepack": "npm run build",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "keywords": [
    "llm",
    "evaluation",
    "ai",
    "testing"
  ],
  "author": "The Focus AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/The-Focus-AI/umwelten.git"
  },
  "homepage": "https://umwelten.thefocus.ai",
  "bugs": {
    "url": "https://github.com/The-Focus-AI/umwelten/issues"
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist/",
    "README.md",
    "LICENSE",
    "env.template"
  ],
  "packageManager": "pnpm@10.6.1",
  "dependencies": {
    "@ai-sdk/google": "^2.0.14",
    "@ai-sdk/openai-compatible": "^1.0.17",
    "@google/generative-ai": "^0.24.1",
    "@openrouter/ai-sdk-provider": "^1.2.0",
    "@types/glob": "^9.0.0",
    "@types/react": "^19.1.13",
    "ai": "^5.0.44",
    "chalk": "^5.6.2",
    "cli-table3": "^0.6.5",
    "commander": "^14.0.1",
    "file-type": "^21.0.0",
    "glob": "^11.0.3",
    "ink": "^6.3.0",
    "ollama-ai-provider-v2": "^1.3.1",
    "ora": "^8.2.0",
    "react": "^19.1.1",
    "tiktoken": "^1.0.22",
    "uuid": "^11.1.0",
    "zod": "^4.1.8",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/dotenv": "^8.2.3",
    "@types/node": "^24.3.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "dotenv": "^16.6.1",
    "eslint": "^9.35.0",
    "prettier": "^3.6.2",
    "tsx": "^4.20.5",
    "typescript": "^5.9.2",
    "vite": "^6.3.6",
    "vitepress": "2.0.0-alpha.12",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
